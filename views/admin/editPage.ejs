<h1 class="login-title author">Edit Post</h1>
<form
  action="/admin/edit/<%=postId%>?_method=PUT"
  method="POST"
  class="login-form"
>
  <label for="title">Title</label>
  <input
    type="text"
    id="title"
    name="title"
    placeholder="Enter title"
    value="<%= data.title %>"
  />
  <label for="body">Body</label>
  <textarea name="body" id="body" rows="10" placeholder="Enter body">
<%= data.body %></textarea
  >

  <div class="submit-container">
    <input type="submit" value="Submit" class="btn-secondary" />
  </div>
</form>
